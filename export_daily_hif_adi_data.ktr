  TO_CHAR(TO_DATE(TO_CHAR(2451544 + utc_date_id), &apos;J&apos;), &apos;yyyy-MM-dd&apos;) AS &quot;date&quot;
, continent_code
, country_code
, SUM(requests) AS &apos;count&apos;
FROM download_requests_by_day b
JOIN dates d ON d.date_id = b.utc_date_id
JOIN download_products p ON p.download_product_id = b.download_product_id
JOIN locations l ON l.location_id = b.location_id
WHERE utc_date_id &gt;= DATEDIFF(&apos;d&apos;, TO_DATE(&apos;1999-12-31&apos;,&apos;YYYY-MM-DD&apos;), CURRENT_DATE - ${DAYS_BACK})
AND request_type = &apos;Download&apos;
AND request_result = &apos;Success&apos;
AND download_type = &apos;manual&apos;
AND p.formatted_name = &apos;firefox&apos;
GROUP BY 1, 2, 3
ORDER BY 1, 2, 3
