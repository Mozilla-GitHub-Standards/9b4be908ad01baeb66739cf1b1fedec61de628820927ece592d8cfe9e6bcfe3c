        <jsScript_name>end</jsScript_name>
        <jsScript_script>var theDay = &quot;&quot; + (new Date()).getUTCDay();
var theDir = getVariable(&quot;OUTPUT_DIR&quot;, &quot;XXX&quot;) + theDay;

&#47;&#47; Set this as a system-wide var
setVariable(&quot;HIF_DAY_OF_WEEK&quot;, theDay, &quot;s&quot;);

var fennec_adi_regions_filename = theDir + &quot;&#47;chart_mobile_adi.json&quot;;
var fennec_adi_countries_filename = theDir + &quot;&#47;chart_mobile_adi_plus_countries.json&quot;;

var adi_regions_filename = theDir + &quot;&#47;chart_desktop_adi.json&quot;;
var adi_countries_filename = theDir + &quot;&#47;chart_desktop_adi_plus_countries.json&quot;;

var uptake_regions_filename = theDir + &quot;&#47;chart_desktop_uptake.json&quot;;
var uptake_countries_filename = theDir + &quot;&#47;chart_desktop_uptake_plus_countries.json&quot;;


&#47;&#47; Run percentages for the final date.
calculatePercentagesForVersions();

var newRow = createRowCopy(getOutputRowMeta().size());
var rowIndex = getInputRowMeta().size();
newRow[rowIndex++] = envelopeToJSON(versionsMap.v_a.envelope, true);
newRow[rowIndex++] = envelopeToJSON(versionsMap.v_a.envelope, false);
newRow[rowIndex++] = adi_regions_filename;
newRow[rowIndex++] = adi_countries_filename;

newRow[rowIndex++] = envelopeToJSON(versionsEnvelope, true);
newRow[rowIndex++] = envelopeToJSON(versionsEnvelope, false);
newRow[rowIndex++] = uptake_regions_filename;
newRow[rowIndex++] = uptake_countries_filename;

newRow[rowIndex++] = envelopeToJSON(versionsMap.fennec.envelope, true);
newRow[rowIndex++] = envelopeToJSON(versionsMap.fennec.envelope, false);
newRow[rowIndex++] = fennec_adi_regions_filename;
newRow[rowIndex++] = fennec_adi_countries_filename;

putRow(newRow);
</jsScript_script>
