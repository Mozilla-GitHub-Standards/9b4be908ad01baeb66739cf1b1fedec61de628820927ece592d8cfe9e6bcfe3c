  <step>
    <name>Query for Firefox ADI by date and country</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>metrics</connection>
    <sql>SELECT
  TO_CHAR(TO_DATE(TO_CHAR(2451544 + utc_date_id), &apos;J&apos;), &apos;yyyy-MM-dd&apos;) AS &quot;date&quot;
, product_name
, continent_code
, country_code
, SUM(requests) AS &apos;v_a&apos;
, SUM(CASE WHEN formatted_version_major = &apos;${V.C}&apos; THEN requests ELSE 0 END) AS &apos;v_c&apos;
, SUM(CASE WHEN formatted_version_major = &apos;${V.P1}&apos; THEN requests ELSE 0 END) AS &apos;v_p1&apos;
, SUM(CASE WHEN formatted_version_major = &apos;${V.P2}&apos; THEN requests ELSE 0 END) AS &apos;v_p2&apos;
, SUM(CASE WHEN formatted_version_major = &apos;${V.P3}&apos; THEN requests ELSE 0 END) AS &apos;v_p3&apos;
FROM blocklist_requests_by_day b
JOIN dates d ON d.date_id = b.utc_date_id
JOIN products p ON p.product_id = b.product_id
JOIN locations l ON l.location_id = b.location_id
WHERE utc_date_id &gt;= DATEDIFF(&apos;d&apos;, TO_DATE(&apos;1999-12-31&apos;,&apos;YYYY-MM-DD&apos;), CURRENT_DATE - ${DAYS_BACK})
AND p.product_name IN (&apos;Firefox&apos;, &apos;Fennec&apos;)
GROUP BY 1, 2, 3, 4
ORDER BY 1, 2, 3, 4
</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
