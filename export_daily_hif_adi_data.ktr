        <jsScript_script>var jsDate = String(date);

if (currDate != jsDate) {
	calculatePercentagesForVersions();
	currDate = jsDate;
	initNewDate(currDate);
}

if (String(product_name) == &quot;Fennec&quot;) {
	accumulateDateData(versionsMap.fennec.currDate, continent_code, country_code, Number(v_a));
} else {
	accumulateDateData(versionsMap.v_a.currDate, continent_code, country_code, Number(v_a));
	accumulateDateData(versionsMap.v_c.currDate, continent_code, country_code, Number(v_c));
	accumulateDateData(versionsMap.v_p1.currDate, continent_code, country_code, Number(v_p1));
	accumulateDateData(versionsMap.v_p2.currDate, continent_code, country_code, Number(v_p2));
	accumulateDateData(versionsMap.v_p3.currDate, continent_code, country_code, Number(v_p3));
}
trans_Status = SKIP_TRANSFORMATION;</jsScript_script>
      </jsScript>      <jsScript>        <jsScript_type>1</jsScript_type>
        <jsScript_name>init</jsScript_name>
        <jsScript_script>var currDate = null;

var versionsMap = {
	&quot;fennec&quot;: {
		&quot;envelope&quot;: {&quot;version&quot;:&quot;all&quot;,&quot;json_data&quot;:[]}
	},
	&quot;v_a&quot;: {
		&quot;envelope&quot;: {&quot;version&quot;:&quot;all&quot;,&quot;json_data&quot;:[]}
	},
	&quot;v_c&quot;: {
		&quot;envelope&quot;: {&quot;version&quot;:getVariable(&quot;V.C&quot;,&quot;v_c&quot;),&quot;json_data&quot;:[]}
	},
	&quot;v_p1&quot;: {
		&quot;envelope&quot;: {&quot;version&quot;:getVariable(&quot;V.P1&quot;,&quot;v_p1&quot;),&quot;json_data&quot;:[]}
	},
	&quot;v_p2&quot;: {
		&quot;envelope&quot;: {&quot;version&quot;:getVariable(&quot;V.P2&quot;,&quot;v_p2&quot;),&quot;json_data&quot;:[]}
	},
	&quot;v_p3&quot;: {
		&quot;envelope&quot;: {&quot;version&quot;:getVariable(&quot;V.P3&quot;,&quot;v_p3&quot;),&quot;json_data&quot;:[]}
	}
};

var versionsEnvelope = {&quot;json_data&quot;:[versionsMap.v_c.envelope,versionsMap.v_p1.envelope,versionsMap.v_p2.envelope,versionsMap.v_p3.envelope]};


function calculatePercentagesForVersions() {
	if (currDate === null) { return; }

	for each (var d in [versionsMap.v_c.currDate, versionsMap.v_p1.currDate, versionsMap.v_p2.currDate, versionsMap.v_p3.currDate]) {
		calculatePercentagesForMap(d.count, d.regions[0]);
		calculatePercentagesForMap(d.count, d.countries[0]);

		d.count = calculatePercentage(d.count, versionsMap.v_a.currDate.count);
	}
}

function calculatePercentagesForMap(total, map) {
	for (var k in map) {
		map[k] = calculatePercentage(map[k], total);
	}
}

function calculatePercentage(numerator, denominator) {
	return (denominator == Number(0) ? Number(0) : Number((numerator &#47; denominator) * 100)).toFixed(4);
}

function initNewDate(date) {
	for each (var v in versionsMap) {
		v.currDate = {&quot;date&quot;:date, &quot;count&quot;:0, &quot;regions&quot;:[{}], &quot;countries&quot;:[{}]};
		v.envelope.json_data.push(v.currDate);
	}
}

function accumulateDateData(dateObj, region, country, count) {
	dateObj.count += count;
	dateObj.regions[0][region] = (dateObj.regions[0][region] || 0) + count;
	dateObj.countries[0][country] = (dateObj.countries[0][country] || 0) + count;
}

function filterCountries(key, value) {
	if (key == &quot;countries&quot;) {
		return undefined;
	}
	return value;
}

function envelopeToJSON(envelope, isFilteringCountries) {
	if (isFilteringCountries) {
		return JSON.stringify(envelope, filterCountries);
	} else {
		return JSON.stringify(envelope);
	}
}
</jsScript_script>
